{
  "$schema": "config.schema.json",
  "name": "numpy",
  "rootFolder": "numpy",
  "github": {
    "url": "https://github.com/mathiasbk1/numpy",
    "branch": "main"
  },
  "saveLocation": "./diagrams/",
  "views": {
    "demoView": {
      "packages": [
        {
          "path": "core",
          "depth": 1
